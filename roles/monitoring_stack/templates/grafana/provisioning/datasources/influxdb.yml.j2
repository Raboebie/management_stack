apiVersion: 1

datasources:
  - name: InfluxDB
    type: influxdb
    access: proxy
    url: http://influxdb:{{ influxdb_port }}
    isDefault: true
    jsonData:
      version: Flux
      organization: {{ influxdb_org }}
      defaultBucket: {{ influxdb_bucket }}
    secureJsonData:
      token: {{ influxdb_admin_token }}
    editable: false
